generator client {
  provider   = "prisma-client-js"
  engineType = "client"
}

datasource db {
  provider = "postgresql"
}

generator dbml {
  provider            = "prisma-dbml-generator"
  output              = "../dbml"
  outputName          = "schema.dbml"
  projectName         = "MiProyecto"
  projectDatabaseType = "PostgreSQL"
  projectNote         = "Descripci√≥n de mi proyecto"
}

enum Status {
  IGNORE
  PROCEED
}

model Users {
  email    String @id @unique
  name     String
  password String
}

model Config {
  id                  Int   @id @default(autoincrement())
  confidenceThreshold Float
  urgentThreshold     Float
}

model Points {
  id           Int    @id @default(autoincrement())
  pt           Float
  alerts       Json
  instructions Json
  status       Status @default(IGNORE)
}
